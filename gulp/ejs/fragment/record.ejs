<!DOCTYPE html>
<html lang="ja">
<head>
    <% include ../common/_head_template.ejs %>

    <!--google material design lite-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-pink.min.css">
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>

    <script src="https://cdn.firebase.com/libs/firebaseui/2.5.1/firebaseui.js"></script>
    <link type="text/css" rel="stylesheet" href="https://cdn.firebase.com/libs/firebaseui/2.5.1/firebaseui.css" />

    <link href="../dist/common.css" rel="stylesheet">
    <link href="style2.css" rel="stylesheet">
</head>
<body>

<% include ../common/_login_ui.ejs %>

<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header mdl-layout--fixed-tabs">
    <div id="post_load">

        <!--header-->
        <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">

            <header class="mdl-layout__header">
                <div class="mdl-layout__header-row">
                    <!-- Title -->
                    <span class="mdl-layout-title">体調を記録</span>
                </div>
                <!-- Tabs -->
                <div id="tab-row">
                    <div>
                        <div id="tab-prev" class="tab-btn-w">
                            <button class="mdl-button mdl-js-button mdl-button--icon">
                                <i class="material-icons">keyboard_arrow_left</i>
                            </button>
                        </div>
                        <div id="tab-center">
                            <div id="tab0">
                                <div class="layout__tab holiday"><span>15日(日)</span></div>
                                <div class="layout__tab"><span>16日(月)</span></div>
                                <div class="layout__tab">
                                    <span>17日(火)</span>
                                    <div class="rounded-circle dot"></div>
                                </div>
                                <div class="layout__tab "><span>18日(水)</span></div>
                                <div class="layout__tab is-active"><span>19日(木)</span></div>
                                <div class="layout__tab"><span>20日(金)</span></div>
                                <div class="layout__tab"><span>21日(土)</span></div>
                            </div>
                            <div id="tab1">
                                <div class="layout__tab"><span>15日(日)</span></div>
                                <div class="layout__tab"><span>16日(月)</span></div>
                                <div class="layout__tab"><span>17日(火)</span></div>
                                <div class="layout__tab"><span>18日(水)</span></div>
                                <div class="layout__tab"><span>19日(木)</span></div>
                                <div class="layout__tab"><span>20日(金)</span></div>
                                <div class="layout__tab"><span>21日(土)</span></div>
                            </div>
                        </div>
                        <div id="tab-next" class="tab-btn-w">
                            <button class="mdl-button mdl-js-button mdl-button--icon">
                                <i class="material-icons">keyboard_arrow_right</i>
                            </button>
                        </div>
                    </div>
                </div>
            </header>

            <div class="mdl-layout__drawer">
                <% include ../common/_drawer.ejs %>
                <% include ../common/DrawerLink/_record.ejs %>
            </div>

            <div class="page-content">
                <!-- Your content goes here -->
                <div id="wrapper">

                    <!-- Page Content -->
                    <div id="page-content-wrapper">
                        <div id="container-fluid">
                            <div id="fab-w-w">
                                <button class="mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-shadow--4dp " id="save" data-toggle="tooltip" data-placement="top" title="テンプレートを保存">
                                    <i class="material-icons">save</i>
                                </button>
                            </div>

                            <div class="card_wrapper">

                                <div class="card-wrapper-i" data-order="1" card-type="1">
                                    <div class="card mix mdl-shadow--2dp">
                                        <h4 class="ele_header">
                                            <i class="fas fa-clock"></i>イベント
                                        </h4>
                                        <ul class="mdl-list">
                                            <li class="mdl-list__item">
                                                <div class="mdl-list__item-primary-content">
                                                    <form action="#" class="time-single">
                                                        <div class="mdl-textfield mdl-js-textfield time is-forcused">
                                                            <input class="mdl-textfield__input" type="text" id="inputId4_0" value="9:00">
                                                            <label class="mdl-textfield__label" for="inputId4_0"></label>
                                                            <span class="mdl-textfield__error">時間がふにふに</span>
                                                        </div>
                                                    </form>
                                                    <div class="mdl-textfield mdl-js-textfield params_title_w">
                                                        <input class="mdl-textfield__input input_eve  params_title" type="text" id="inputId4_1" value="朝薬">
                                                        <label class="mdl-textfield__label" for="inputId4_1"></label>
                                                        <span class="mdl-textfield__error">項目名を入力してください</span>
                                                    </div>
                                                </div>
                                                <div class="mdl-list__item-secondary-content">
                                                    <button class="mdl-button mdl-js-button mdl-button--icon">
                                                        <i class="fas fa-times fa-sm"></i>
                                                    </button>
                                                </div>
                                            </li>

                                            <li class="mdl-list__item">
                                                <span class="mdl-list__item-primary-content">
                                                    <span class="time-multi">
                                                        <span class="mdl-textfield mdl-js-textfield time start">
                                                            <input class="mdl-textfield__input" type="text" id="inputId4_2" value="9:00">
                                                            <label class="mdl-textfield__label" for="inputId4_2"></label>
                                                            <span class="mdl-textfield__error">時間がふにふに</span>
                                                        </span>

                                                        <span class="arrow"><i class="fas fa-angle-double-right"></i></span>

                                                        <span class="mdl-textfield mdl-js-textfield time end">
                                                            <input class="mdl-textfield__input" type="text" id="inputId4_3" value="16:00">
                                                            <label class="mdl-textfield__label" for="inputId4_3"></label>
                                                            <span class="mdl-textfield__error">時間がふにふに</span>
                                                        </span>
                                                    </span>

                                                    <span class="mdl-textfield mdl-js-textfield params_title_w">
                                                        <input class="mdl-textfield__input input_eve  params_title" type="text" id="inputId4_4" value="朝薬">
                                                        <label class="mdl-textfield__label mdl-pre-upgrade" for="inputId4_4"></label><span class="mdl-textfield__error mdl-pre-upgrade">項目名を入力してください</span>
                                                    </span>
                                                </span>
                                                <span class="mdl-list__item-secondary-content">
                                                    <button class="mdl-button mdl-js-button mdl-button--icon">
                                                        <i class="fas fa-times fa-sm"></i>
                                                    </button>
                                                </span>
                                            </li>
                                        </ul>
                                        <div class="add-button-w">
                                            <button class="mdl-button mdl-js-button mdl-button--icon">
                                                <i class="material-icons">add_circle</i>
                                            </button>
                                        </div>
                                    </div>
                                </div>

                                <div class="card-wrapper-i" data-order="1">
                                    <div class="card mix mdl-shadow--2dp">
                                        <h4 class="ele_header">
                                            <i class="fas fa-tags"></i>症状・セルフケア
                                        </h4>
                                        <div class="tag_pool">
                                            <span class="mdl-chip mdl-chip--deletable">
                                                <span class="mdl-chip__text">神経質</span>
                                                <button type="button" class="mdl-chip__action"><i class="material-icons">cancel</i></button>
                                            </span>
                                            <button class="mdl-button mdl-js-button mdl-button--icon tag-add-btn" data-tag-order="1000" data-toggle="tooltip" data-placement="top" title="" data-original-title="項目を追加" data-upgraded=",MaterialButton">
                                                <i class="material-icons">add_circle</i>
                                            </button>
                                        </div>
                                    </div>
                                </div>

                                <div class="card-wrapper-i" data-order="1">
                                    <div class="card mix mdl-shadow--2dp">
                                        <h4 class="ele_header">
                                            <i class="fas fa-edit"></i>備考
                                        </h4>
                                        <form action="#" class="comment_form">
                                            <div class="mdl-textfield mdl-js-textfield comment">
                                                <textarea class="mdl-textfield__input comment_ta" type="text" rows="3" id="comment"></textarea>
                                                <label class="mdl-textfield__label" for="comment">Text...</label>
                                                <span class="mdl-textfield__error"></span>
                                            </div>
                                        </form>
                                    </div>
                                </div>

                                <div class="card-wrapper-i" data-order="1" card-type="4">
                                    <div class="card mix mdl-shadow--2dp">
                                        <h4 class="ele_header">
                                            <i class="fas fa-edit"></i>備考
                                        </h4>
                                        <ul class="mdl-list">
                                            <li class="mdl-list__item">
                                                 <span class="params_check">
                                                    <label class="mdl-checkbox mdl-js-checkbox mdl-js-ripple-effect mdl-js-ripple-effect--ignore-events is-checked" for="checkId4_0">
                                                        <input type="checkbox" id="checkId4_0" class="mdl-checkbox__input">
                                                        <span class="mdl-checkbox__focus-helper"></span>
                                                        <span class="mdl-checkbox__box-outline">
                                                            <span class="mdl-checkbox__tick-outline"></span>
                                                        </span>
                                                        <span class="mdl-checkbox__ripple-container mdl-js-ripple-effect mdl-ripple--center">
                                                            <span class="mdl-ripple"></span>
                                                        </span>
                                                    </label>
                                                </span>
                                                <p class="slider_wrapper params_slider">
                                                    <div class="mdl-slider__container">
                                                        <input class="mdl-slider mdl-js-slider" type="range" min="0" max="5" value="3" step="1" data-toggle="popover">
                                                    </div>
                                                </p>
                                                <span class="mdl-list__item-primary-content">
                                                    <form action="#">
                                                        <div class="mdl-textfield mdl-js-textfield params_title_w">
                                                            <input class="mdl-textfield__input input_eve  params_title" type="text" id="inputId4_0" value="朝薬">
                                                            <label class="mdl-textfield__label mdl-pre-upgrade" for="inputId4_0"></label><span class="mdl-textfield__error mdl-pre-upgrade">項目名を入力してください</span>
                                                        </div>
                                                    </form>
                                                </span>
                                                <span class="mdl-list__item-secondary-content">
                                                    <button class="mdl-button mdl-js-button mdl-button--icon">
                                                        <i class="material-icons">clear</i>
                                                    </button>
                                                </span>
                                            </li>
                                            <li class="mdl-list__item">
                                                 <span class="params_check">
                                                    <label class="mdl-checkbox mdl-js-checkbox mdl-js-ripple-effect mdl-js-ripple-effect--ignore-events is-checked" for="checkId4_0">
                                                        <input type="checkbox" id="checkId4_0" class="mdl-checkbox__input">
                                                        <span class="mdl-checkbox__focus-helper"></span>
                                                        <span class="mdl-checkbox__box-outline">
                                                            <span class="mdl-checkbox__tick-outline"></span>
                                                        </span>
                                                        <span class="mdl-checkbox__ripple-container mdl-js-ripple-effect mdl-ripple--center">
                                                            <span class="mdl-ripple"></span>
                                                        </span>
                                                    </label>
                                                </span>
                                                <p class="slider_wrapper params_slider">
                                                    <input class="mdl-slider mdl-js-slider" type="range" min="0" max="5" value="3" step="1" data-toggle="popover">
                                                </p>
                                                <span class="mdl-list__item-primary-content">
                                                    <form action="#">
                                                        <div class="mdl-textfield mdl-js-textfield params_title_w">
                                                            <input class="mdl-textfield__input input_eve  params_title" type="text" id="inputId4_0" value="朝薬">
                                                            <label class="mdl-textfield__label" for="inputId4_0"></label><span class="mdl-textfield__error mdl-pre-upgrade">項目名を入力してください</span>
                                                        </div>
                                                    </form>
                                                </span>
                                                <span class="mdl-list__item-secondary-content">
                                                    <button class="mdl-button mdl-js-button mdl-button--icon">
                                                        <i class="material-icons">clear</i>
                                                    </button>
                                                </span>
                                            </li>
                                        </ul>
                                        <div class="add-button-w">
                                            <button class="mdl-button mdl-js-button mdl-button--icon">
                                                <i class="material-icons">add_circle</i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div id="right-column">
                                <div id="right-spacer"></div>
                                <div>
                                    <ul id="right-ul">
                                        <li>
                                            <a href="#hogehoge">最初のコンテンツ</a>
                                        </li>
                                        <li>
                                            <a href="#hogehoge">次のコンテンツ</a>
                                        </li>
                                        <li>
                                            <a href="#hogehoge">症状・セルフケア</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>

                        <div id="page-load-prg" class="mdl-spinner mdl-js-spinner is-active"></div>
                    </div>
                </div>
            </div>

            <% include ../common/_footer.ejs %>
        </div>
    </div>
</div>

<!-- Bootstrap core JavaScript -->
<script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/js/bootstrap.min.js" integrity="sha384-a5N7Y/aK3qNeh15eJKGWxsqtnX/wWdSZSKp+81YjTmS15nvnvxKHuzaWwXHDli+4" crossorigin="anonymous"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.20.1/moment.min.js"></script>
<script defer src="https://use.fontawesome.com/releases/v5.0.1/js/all.js"></script>
<script src="../dist/bootstrap-notify.min.js"></script>

<script src="https://www.gstatic.com/firebasejs/4.12.0/firebase-app.js"></script>
<script src="https://www.gstatic.com/firebasejs/4.12.0/firebase-auth.js"></script>
<script src="https://www.gstatic.com/firebasejs/4.12.0/firebase-database.js"></script>

<script src="../dist/common.js"></script>
<script src="app2.js"></script>

<% include ../common/_no_script.ejs %>

</body>
</html>